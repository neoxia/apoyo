  },
  "scripts": {
    "lint": "npx eslint '{src,tests}/**/*.{ts,tsx,json,yml}'",
    "prettier": "prettier --list-different \"./{src,tests,examples,scripts}/**/*.ts\"",
    "fix-prettier": "prettier --write \"./{src,tests,examples,scripts}/**/*.ts\"",
    "test": "npm run lint && npm run prettier && npm run test:unit",
