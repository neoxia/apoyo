{
  "name": "@ordinary/utils",
  "version": "0.0.1",
  "description": "My description",
  "main": "lib/index.js",
  "module": "es6/index.js",
  "typings": "lib/index.d.ts",
  "sideEffects": false,
  "author": "Sascha Braun",
  "license": "MIT",
  "tags": [
    "utils",
    "functional",
    "fp",
    "pipable",
    "typescript"
  ],
  "keywords": [
    "utils",
    "functional",
    "fp",
    "pipable",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "https://git.neoxia.com/typescript/ordinary-utils"
  },
  "scripts": {
    "lint": "npx eslint '{src,tests}/**/*.{ts,tsx,json,yml}'",
    "prettier": "npx prettier --list-different \"./{src,tests,examples,scripts}/**/*.ts\"",
    "fix-prettier": "npx prettier --write \"./{src,tests,examples,scripts}/**/*.ts\"",
    "test": "npm run lint && npm run prettier && npm run test:unit",
    "test:unit": "npx jest --clearCache && jest --verbose --runInBand",
    "clean": "npx rimraf ./dist",
    "prebuild": "npm run clean",
    "build": "npx tsc -p ./tsconfig.build.json && npx tsc -p ./tsconfig.build-es6.json && npx ts-node scripts/build",
    "postbuild": "npx prettier --loglevel=silent --write \"./dist/**/*.ts\"",
    "prepublishOnly": "npx ts-node scripts/pre-publish",
    "prerelease": "npm run build && npm run test",
    "release": "npx ts-node scripts/release",
    "postinstall": "husky install"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/jest": "22.2.2",
    "@types/node": "^12.6.8",
    "@types/prettier": "1.10.0",
    "@types/underscore": "^1.10.24",
    "@typescript-eslint/eslint-plugin": "^4.15.0",
    "@typescript-eslint/parser": "^4.15.0",
    "eslint": "^7.19.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-prettier": "^3.3.1",
    "fast-check": "^1.25.1",
    "glob": "^7.1.6",
    "husky": "^5.0.9",
    "jest": "^26.4.2",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1",
    "rimraf": "2.6.2",
    "ts-jest": "^26.3.0",
    "ts-node": "^8.0.2",
    "typescript": "^4.1.2"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "npx eslint --fix"
    ]
  }
}
